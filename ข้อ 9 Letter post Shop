    private void btnClearActionPerformed(java.awt.event.ActionEvent evt) {                                         
        txtWeightpost.setText("");
        tarResult.setText("");
    }                                        

    private void btnExitActionPerformed(java.awt.event.ActionEvent evt) {                                        
        int option = JOptionPane.showConfirmDialog(rootPane, "Do you want to Exit ?");
        if (option == JOptionPane.YES_OPTION) {
            System.exit(0);
        }
    }                                       

    private void btnCalculateActionPerformed(java.awt.event.ActionEvent evt) {                                             
        float wei,result=0;
        wei=Float.parseFloat(txtWeightpost.getText());
        if (rdlNomal.isSelected()) {
            if ((wei >1000)&&(wei<=2000)) {
                tarResult.setText("45");
            } else if ((wei >500)&&(wei<=1000)) {
                 tarResult.setText("25");
            } else if ((wei >250)&&(wei<=500)) {
                 tarResult.setText("15");
            } else if ((wei >100)&&(wei<=250)) {
                 tarResult.setText("9");
            } else if ((wei >20)&&(wei<=250)) {
                tarResult.setText("5");
            } else {
                 tarResult.setText("3");
            }
        }else if (rdlEms.isSelected()) {
            if ((wei >1000)&&(wei<=2000)) {
                tarResult.setText("77");
            }else if (((wei >500)&&(wei<=1000))) {
                tarResult.setText("62");
            } else if (((wei >250)&&(wei<=500))) {
                tarResult.setText("47");
            } else if (((wei >100)&&(wei<=250))) {
                tarResult.setText("37");
            } else if ((wei >20)&&(wei<=250)) {
                tarResult.setText("32");
            } else  {
                tarResult.setText("27");
        }
        }else if (rdlRegister.isSelected()) {
             if ((wei >1000)&&(wei<=2000)) {
                tarResult.setText("58");
            }else if (((wei >500)&&(wei<=1000))) {
                tarResult.setText("38");
            } else if (((wei >250)&&(wei<=500))) {
                tarResult.setText("28");
            } else if (((wei >100)&&(wei<=250))) {
                tarResult.setText("22");
            } else if ((wei >20)&&(wei<=250)) {
                tarResult.setText("18");
            } else  {
                tarResult.setText("16");
        }
      }
       
    }                                            
  
